.slick-arrow.slick-disabled {
    opacity: 0.2;
    pointer-events: none;
}

#imageGrid {
    position: relative;
    width: 100%;
    height: 80vh;
}

#imageGrid a {
    position: absolute;
    display: block;
    transition: transform 0.3s ease;
    z-index: 1;
    opacity: 0;
    transform: scale(0.9);
    animation: fadeInScale 0.6s ease-out forwards;

}


@keyframes fadeInScale {
    to {
        opacity: 1;
        transform: scale(1);
    }
}

#imageGrid a:hover {
    transform: scale(1.05) !important;
    transition: transform 0.3s ease;
}



