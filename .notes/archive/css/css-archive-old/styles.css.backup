    backdrop-filter: blur(5px);
    transition: background-color 0.3s ease-in-out, padding 0.3s ease-in-out;
    will-change: background-color, padding;
}

/* Scrolled state */
#mainNavbar.scrolled {
    background-color: rgba(0, 0, 0, 0.9);
    padding: 0.75rem 0;
    box-shadow: 0 2px 15px rgba(0, 0, 0, 0.2);
}

/* Navbar container */
#mainNavbar .container {
    max-width: 100%;
    padding: 0 2rem;
}

/* Navbar brand/logo */
#mainNavbar .navbar-brand {
    padding: 0;
    margin: 0;
    display: flex;
    align-items: center;
}

#mainNavbar .navbar-logo {
    height: 50px;
    width: auto;
    transition: all 0.3s ease-in-out;
}

/* Navbar toggler (mobile menu button) */
#mainNavbar .navbar-toggler {
    border: none;
    padding: 0.5rem;
    transition: all 0.3s ease-in-out;
    background: transparent;
    cursor: pointer;
}

#mainNavbar .navbar-toggler:focus {
    outline: none;
    box-shadow: none;
}

/* Custom hamburger menu */
.hamburger {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    width: 24px;
    height: 18px;
    cursor: pointer;
}

.hamburger-line {
    display: block;
    width: 100%;
    height: 2px;
    background-color: var(--main-orange);
    border-radius: 2px;
    transition: all 0.3s ease-in-out;
}

/* Active state for hamburger (transforms to X) */
#mainNavbar .navbar-toggler[aria-expanded="true"] .hamburger-line:nth-child(1) {
    transform: translateY(8px) rotate(45deg);
}

#mainNavbar .navbar-toggler[aria-expanded="true"] .hamburger-line:nth-child(2) {
    opacity: 0;
}

#mainNavbar .navbar-toggler[aria-expanded="true"] .hamburger-line:nth-child(3) {
    transform: translateY(-8px) rotate(-45deg);
}

