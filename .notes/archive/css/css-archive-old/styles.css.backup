/* Custom orange toggler icon for Bootstrap navbar */
.navbar-toggler {
    border: none;
    outline: none;
    box-shadow: none;
    padding: 0.25rem 0.5rem;
}

.navbar-toggler-icon {
    background-image: none;
    /* Inline SVG in HTML is now the only icon source */
}

.navbar-toggler-icon svg {
    width: 2rem;
    height: 2rem;
    display: block;
    margin: auto;
}

.navbar-toggler .icon-hamburger { display: block; }
.navbar-toggler .icon-close { display: none; }
.navbar-toggler.open .icon-hamburger { display: none; }
.navbar-toggler.open .icon-close { display: block; }

.video-wrapper video {
    width: 100%;
